<%= form_for @song do |f| %>
    <%= f.label "Title"  %>
    <%= f.text_field :title  %>
    <br>
    <%= f.label "Genre" %>
    <%= f.text_field :genre  %>
    <br>
    <%= f.label "Artist" %>
    <%= f.text_field :artist_name  %>
    <br>
    <%= f.label "Release Year" %>
    <%= f.number_field :release_year  %>
     <br>
    <%= f.label "Released ?" %>
    
    <%= f.check_box :released, {}, 'true', 'false'%>
    <br>
    <%= f.submit  %>
<% end %>